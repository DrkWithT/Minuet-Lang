# test nested while loops #

fun loop_de_loop: [n] => {
    def i = 0
    def j = 0
    def ans = 0

    while i < n {
        while j < n {
            ans = ans + 1
            j = j + 1
        }

        i = i + 1
        j = 0
    }

    return ans
}

fun main: [] => {
    def count = loop_de_loop(5)

    print(count)

    if count != 25 {
        return 1
    }

    return 0
}
