# sum from 1 to N by loop #

import "./stdlib/stdio.mnl"

fun sumLoop: [n] => {
    def count = n
    def ans = 0

    while count > 0 {
        ans = ans + count
        count = count - 1
    }

    return ans
}

fun main: [] => {
    def ans = sumLoop(10)

    print(ans)

    if ans != 55 {
        return 1
    }

    return 0
}