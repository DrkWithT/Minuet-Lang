fun isPrime: [n] => {
    def factor = n / 2
    def ans = true

    while factor > 1 {
        if n % factor == 0 {
            ans = false
            break
        }

        factor = factor - 1
    }

    return ans
}

fun main: [] => {
    if isPrime(4) == false {
        return 0
    }

    return 1
}
