# naive recursive fibonacci

fun fibo: [n] => {
    match n {
        pat 0 -> {
            0
        }
        pat 1 -> {
            1
        }
        _ -> {
            fibo(n - 1) + fibo(n - 2)
        }
    }
}

fun main: [args] => {
    def arg_n := parseInt(args.0)
    def ans := fibo(arg_n)

    print(ans)

    assertTrue(ans > 0)
}
