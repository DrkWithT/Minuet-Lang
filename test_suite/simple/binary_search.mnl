# search sorted tuple #

import "./stdlib/stdio.mnl"

fun main: [] => {
    def nums = [1, 2, 3, 4, 5]
    def target = 1

    def low = 0
    def high = 4
    def mid = 0
    def pos = (0 - 1)
    def temp = 0

    while low < high {
        mid = (low + high) / 2
        temp = nums.mid

        if temp == target {
            pos = mid
            break
        }

        if temp > target {
            high = mid - 1
        } else {
            low = mid + 1
        }
    }

    print(pos)

    if pos == (0 - 1) {
        return 1
    }

    return 0
}